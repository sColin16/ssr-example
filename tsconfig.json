{
  "compilerOptions": {
    "lib": ["es2022", "dom"],
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "outDir": "dist",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "noEmit": true,
    "esModuleInterop": true,
  },
  "include": ["src/**/*"],
}
